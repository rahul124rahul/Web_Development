    <strong><i>RAHUL'S FRIENDS : </i></strong>

      <!-- This is ordered type of list -->
    <ol type="1">   <!-- This is the lower case roman type counting -->

        <li><ins>NEW YORK</ins><DEL>Tokyo</DEL></li>    <!-- Here insert and delete tags used ********** -->

        <li>Nairobi</li>        <!-- In this we can use 'I','a','A','1' any of this also -->
        <li>Riyo</li>
        <li>Birlin</li>
